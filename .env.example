# Copy to .env and set values. Used by docker-compose.nextcloud.yml and other compose files.
# Do not commit .env (secrets).

# --- Nextcloud + HattieBot (docker-compose.nextcloud.yml) ---
POSTGRES_DB=nextcloud
POSTGRES_USER=nextcloud
POSTGRES_PASSWORD=changeme

NEXTCLOUD_ADMIN_USER=admin
NEXTCLOUD_ADMIN_PASSWORD=changeme
# Include "nextcloud" so HattieBot's bootstrap (Host: nextcloud) gets 200 from status.php
NEXTCLOUD_TRUSTED_DOMAINS=localhost nextcloud
NEXTCLOUD_TALK_BOT_SECRET=changeme-min-40-chars-for-talk-bot-secret
NEXTCLOUD_TALK_BOT_NAME=HattieBot
NEXTCLOUD_TALK_BOT_URL=http://hattiebot:8080/webhook/talk

OPENROUTER_API_KEY=
HATTIEBOT_MODEL=moonshotai/kimi-k2.5
HATTIEBOT_BOT_NAME=HattieBot
HATTIEBOT_AUDIENCE=your-audience
HATTIEBOT_PURPOSE=your-purpose

# --- Optional (HattieBot deploy / demo) ---
# HATTIEBOT_CONFIG_DIR=/data
# EMBEDDING_SERVICE_URL=
# EMBEDDING_SERVICE_API_KEY=
